
<div class="card checkoutformpostion">
    <form action="#" method="POST">
        <label for="">Title</label>
        <div class="form-group">
          <input
            type="text"
            name="title"
            id="title"
            class="form-control checkoutform"
            value="<%= movie.title %>"
            readonly
          />
        </div>
        <div class="form-group">
            <input
              type="text"
              name="director"
              id="director"
              class="form-control checkoutform"
              value="<%= movie.director %>"
              readonly
            />
          </div>
          <div class="form-group">
            <input
              type="text"
              name="price"
              id="price"
              class="form-control checkoutform"
              value="$<%= movie.price %>.00"
              readonly
            />
          </div>
          <br>
        <button class="btn btn-lg btn-primary btn-block" type="submit">Proceed to Purchase</button>
      </form>
</div>